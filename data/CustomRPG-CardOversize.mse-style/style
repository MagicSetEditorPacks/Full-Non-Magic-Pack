mse version: 2.0.0
game: CustomRPG
short name: Oversize
full name: (19.2cm x 13.2cm)
icon: icon.png
depends on: CustomRPG.mse-game 1.0
depends on: CustomRPG.mse-include 1.0
depends on: CustomRPG.mse-symbol-font 1.0
card background: white
#card width: 1152
#card height: 792
#card dpi: 152.4
card width: 1920
card height: 1320
card dpi: 254

############################################################## Scripts
init script:
	dpi := 254

	##### Category
	cardlist_display1 := {card.Category}
	##### Type
	cardlist_display2 := {card.Type}
	##### Subtype/Info
	cardlist_display3 := {number_filter(card.Info1)}
	##### Misc/Info
	cardlist_display4 := {number_filter(card.Info2)}
	##### Background
	cardlist_display5 := {card.Background}

	overlay_style := {
	if styling.Statboxes == 12 then if styling.GaugesStyle == "type1" then "/CustomRPG.mse-include/card-overlay3g.png" else if styling.GaugesStyle == "type2" then "/CustomRPG.mse-include/card-overlay4g.png" else "/CustomRPG.mse-include/card-overlay2g.png"
	else if styling.Statboxes == 10 then if styling.GaugesStyle == "type1" then "/CustomRPG.mse-include/card-overlay3f.png" else if styling.GaugesStyle == "type2" then "/CustomRPG.mse-include/card-overlay4f.png" else "/CustomRPG.mse-include/card-overlay2f.png"
	else if styling.Statboxes == 8 then if styling.GaugesStyle == "type1" then "/CustomRPG.mse-include/card-overlay3e.png" else if styling.GaugesStyle == "type2" then "/CustomRPG.mse-include/card-overlay4e.png" else "/CustomRPG.mse-include/card-overlay2e.png"
	else if styling.Statboxes == 6 then if styling.GaugesStyle == "type1" then "/CustomRPG.mse-include/card-overlay3d.png" else if styling.GaugesStyle == "type2" then "/CustomRPG.mse-include/card-overlay4d.png" else "/CustomRPG.mse-include/card-overlay2d.png"
	else if styling.Statboxes == 4 then if styling.GaugesStyle == "type1" then "/CustomRPG.mse-include/card-overlay3c.png" else if styling.GaugesStyle == "type2" then "/CustomRPG.mse-include/card-overlay4c.png" else "/CustomRPG.mse-include/card-overlay2c.png"
	else if styling.Statboxes == 2 then if styling.GaugesStyle == "type1" then "/CustomRPG.mse-include/card-overlay3b.png" else if styling.GaugesStyle == "type2" then "/CustomRPG.mse-include/card-overlay4b.png" else "/CustomRPG.mse-include/card-overlay2b.png"
	else if styling.GaugesStyle == "type1" then "/CustomRPG.mse-include/card-overlay3a.png" else if styling.GaugesStyle == "type2" then "/CustomRPG.mse-include/card-overlay4a.png" else "/CustomRPG.mse-include/card-overlay2a.png"
	}

