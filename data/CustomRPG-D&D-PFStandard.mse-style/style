############################################################## Main Info
	Name:
		top: {20*dpi/254}
		left: {100*dpi/254}
		width: {435*dpi/254}
		height: {50*dpi/254}
		alignment: center middle stretch if-overflow
		font: 
			name: {styling.NameFont}
			size: {28*dpi/254}
			weight: bold
			color: {styling.NameColor}
		allow formating: false

	Picture:
		top: {if contains(styling.CardType, match: "monster") then 640*dpi/254 else if contains(styling.CardType, match: "item") or contains(styling.CardType, match: "genericD") then 120*dpi/254 else 0}
		left: {if contains(styling.CardType, match: "monster") then 415*dpi/254 else if contains(styling.CardType, match: "item") or contains(styling.CardType, match: "genericD") then 415*dpi/254 else 0}
		width: {if contains(styling.CardType, match: "monster") then 200*dpi/254 else if contains(styling.CardType, match: "item") or contains(styling.CardType, match: "genericD") then 200*dpi/254 else if contains(styling.CardType, match: "cardback") then 635*dpi/254 else 0}
		height: {if contains(styling.CardType, match: "monster") then 160*dpi/254 else if contains(styling.CardType, match: "item") or contains(styling.CardType, match: "genericD") then 160*dpi/254 else if contains(styling.CardType, match: "cardback") then 886*dpi/254 else 0}

	Watermark:
		top: {if contains(styling.CardType, match: "monster") then 265*dpi/254 else 400*dpi/254}
		left: {if contains(styling.CardType, match: "monster") then 135*dpi/254 else 180*dpi/254}
		width: {280*dpi/254}
		height: {280*dpi/254}
		visible: {if styling.Watermark == "empty" then false else true}
		z index: -1
		render style: image
		popup style: dropdown
		choice images:
			default:	{set_combine(input: combine_blend(image1: "/CustomRPG.mse-include/watermark-overlay.png", image2: "/CustomRPG.mse-include/" + to_string(styling.Watermark) + ".png", combine: "overlay"), combine: "normal")}

