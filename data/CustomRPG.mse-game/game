############################################################## Basic fields
card field:
	type: text
	name: Name
	identifying: true
	show statistics: false
	card list visible: true
	card list name: Name
	card list column: 0
	description: Name of the card

card field:
	type: image
	name: Picture
	show statistics: false
	card list allow: false
	description: Picture of the card

card field:
	type: text
	name: Category
	show statistics: false
	card list allow: false
	description: Primary type of the card

card field:
	type: text
	name: Type
	show statistics: false
	card list allow: false
	description: Primary subtype/keyword of the card

card field:
	type: text
	name: Category-Type
	show statistics: false
	card list allow: false
	save value: false
	description: Primary type and subtype(s) of the card
	script: combined_editor(field1: card.Category, separator: " — ", field2: card.Type, soft_before_empty: true, hide_when_empty: false)

card field:
	type: text
	name: Generic
	show statistics: false
	card list allow: false
	description: Secondary type of the card

card field:
	type: text
	name: Subtype
	show statistics: false
	card list allow: false
	description: Secondary subtype/keyword of the card

card field:
	type: text
	name: Generic-Subtype
	show statistics: false
	card list allow: false
	save value: false
	description: Secondary type and subtype(s) of the card
	script: combined_editor(field1: card.Generic, separator: " — ", field2: card.Subtype, soft_before_empty: true, hide_when_empty: false)

card field:
	type: text
	name: Description
	multi line: true
	show statistics: false
	card list allow: false
	description: Primary textbox of the card

card field:
	type: text
	name: Rules
	multi line: true
	show statistics: false
	card list allow: false
	description: Secondary textbox of the card

card field:
	type: text
	name: Description-Rules
	multi line: true
	show statistics: false
	card list allow: false
	save value: false
	description: Combined textbox of the card
	script: combined_editor(field1: card.Description, separator: "<line>\n</line>", field2: card.Rules)

