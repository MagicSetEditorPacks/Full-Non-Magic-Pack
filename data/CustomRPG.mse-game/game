mse version: 2.0.0
short name: CustomRPG
full name: Krevett62
icon:icon.png
depends on: CustomRPG.mse-include 1.0
position hint: 0
version: 1.0

# Combination of Zio's Custom RPG frames and erf_beto DnDStatCards and 
# https://www.enworld.org/resources/zios-magic-set-editor-card-templates-for-rpgs.1591/
# https://www.enworld.org/threads/monster-cards.297579/
# beto credits:
# Hacked by erf_beto :D
# Special thanks to knight_otu, ander00, lord tirian and others from enworld.org, for inspiration and/or prior design/programming. :)
# This is my first attempt at a MSE template. There's probably some lines I'm not using, I know. I'm still figuring it out :P


############################################################## Scripts
init script:
	number_filter := filter_text@(match: "[0123456789]")

	action_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Action>{input}</word-list-Action>" }

	components_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Components>{input}</word-list-Components>" }

	range_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Range>{input}</word-list-Range>" }

	duration_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Duration>{input}</word-list-Duration>" }

	area_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Area>{input}</word-list-Area>" }

	attack_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Attack>{input}</word-list-Attack>" }

	class_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Class>{input}</word-list-Class>" }

	role_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Role>{input}</word-list-Role>" }

	monstertype_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Monstertype>{input}</word-list-Monstertype>" }

	powertype_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Powertype>{input}</word-list-Powertype>" }

	save_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Save>{input}</word-list-Save>" }

	rarity_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Rarity>{input}</word-list-Rarity>" }

	value_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Value>{input}</word-list-Value>" }

	power_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Power>{input}</word-list-Power>" }

	school_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-School>{input}</word-list-School>" }

	keyword_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Keyword>{input}</word-list-Keyword>" }

	size_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Size>{input}</word-list-Size>" }

	monster_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Monster>{input}</word-list-Monster>" }

	item_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Item>{input}</word-list-Item>" }

	slot_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Slot>{input}</word-list-Slot>" }

	generic_filter := tag_remove_rule(tag: "<word-list-") + { "<word-list-Generic>{input}</word-list-Generic>" }

	cardlist_display1 := {""}
	cardlist_display2 := {""}
	cardlist_display3 := {""}
	cardlist_display4 := {""}
	cardlist_display5 := {""}

	## beto statcards

	card_number := {
		position (
			of: card
			in: set
			order_by: {card.StatRole + card.StatName }
		) + 1
	}
	############################################################## The text box
	
	# Filters for the text box
	# the rule text filter
	text_filter :=
		# step 1 : remove all automatic tags
		tag_remove_rule(tag: "<sym-auto>") +
		tag_remove_rule(tag: "<i-auto>")   +
		tag_remove_rule(tag: "<b-auto>")   +
		# step 2 : expand shortcut words ~ and CARDNAME
		replace_rule(
			match: "~|~THIS~|CARDNAME",
			in_context: "(^|[[:space:]]|\\()<match>", # TODO: Allow any punctuation before
			replace: "<atom-cardname></atom-cardname>"
			) +
		# step 3 : fill in atom fields
		tag_contents_rule(
			tag: "<atom-cardname>",
			contents: { if card_name=="" then "CARDNAME" else card_name }
			) +
		# step 4 : automatic bolding and capitalization
		replace_rule(
			match: "[Aa]ttacks|[Ll]imited [Pp]owers|[Aa]bilities|[Ss]kills",
			in_context: "<match>[:]",
			replace: to_upper) +
		replace_rule(
			match: "ATTACKS:|LIMITED POWERS:|ABILITIES:|SKILLS:",
			replace: "<b-auto>&</b-auto>") +
		#step 5 : Replace dot
		replace_rule(
			match: "\\*",
			replace: "•") +
		#step 5 : Attack symbols
		replace_rule(
			match: "[(][aAcCmMrRFBDU123456][)]",
			replace: "<sym-auto>&</sym-auto>") +
		curly_quotes	
	upper_filter :=
		tag_remove_rule(tag: "<size:") +
		tag_remove_rule(tag: "</size") +
		tag_remove_rule(tag: "<SIZE:") +
		tag_remove_rule(tag: "</SIZE") +
		to_upper
	name_filter :=
		tag_remove_rule(tag: "<size") +
		tag_remove_rule(tag: "</size") +
		tag_remove_rule(tag: "<SIZE:") +
		tag_remove_rule(tag: "</SIZE") +
		to_upper +
		replace_rule(match: "[A-Z]", in_context:"^<match>| <match>", replace: "<size:16>&</size>")
	ability_filter :=
		tag_remove_rule(tag: "<size") +
		tag_remove_rule(tag: "</size") +
		replace_rule(match: "[(]*[-–+]*[0-9]*[)]", in_context:"^<match>| <match>", replace: "<size:7>&</size>")
	type_filter :=
		#tag_remove_rule(tag: "<word-list-") +
		#replace_rule(match: " </soft>$", replace: "") + # remove trailing soft space
		#tag_remove_rule(tag: "<soft") +
		to_upper #+ 
		#{
			#"<word-list-Sizes>{ only_first(input) }</word-list-Sizes>"+" "+"<word-list-Origins>{ only_second(input) }</word-list-Origins>"+" "+"<word-list-Types>{ only_third(input) }</word-list-Types>" +" "+ only_subtypes(input) 
		#}
	
	# all sub types, for word list
	#only_subtypes  := filter_rule(match:"[(].*")
	#only_first     := replace_rule(match:" [^\n]*", replace:"")
	#only_second    := replace_rule(match:"^[^ ]* ?", replace:"") + replace_rule(match:" [^\n]*", replace:"")
	#only_third     := replace_rule(match:"^[^ ]* ?", replace:"") + replace_rule(match:"^[^ ]* ?", replace:"") + replace_rule(match:".\\([^\n]*", replace:"")


