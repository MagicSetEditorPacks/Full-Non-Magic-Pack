############################################################## Auto replace
auto replace:
	match: -- 
	replace: — 
auto replace:
	match: ** 
	replace: ✦ 
auto replace:
	match: (CR)
	replace: ©
auto replace:
	match: AE
	replace: Æ
	whole word: false
auto replace:
	match: TM
	replace: ™
	whole word: false
auto replace:
	# note the spaces
	match:
		 - 
	replace:
		 — 

