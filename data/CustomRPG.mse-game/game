############################# Name line
card field:
	type: text
	name: StatName
	card list visible: true
	script:
		if set.use_name_filter then
			name_filter(input: value)
		else
			upper_filter(input: value)
	card list column: 1
	identifying: true
	show statistics: false
	description: The name of the monster.
card field:
	type: text
	name: StatRole
	position hint: 1
	script: upper_filter(input: value)
	card list visible: true
	card list column: 2
	description: The role, level, and status of the monster.
card field:
	type: text
	name: xp
	position hint: 1
	script: upper_filter(input: value)
	card list visible: true
	card list column: 2
	description: The xp of the monster.
card field:
	type: text
	name: StatType
	position hint: 1
	script: type_filter(input: value)
	card list visible: true
	card list column: 3
	description: The size, origin, type, and subtype of the monster.

