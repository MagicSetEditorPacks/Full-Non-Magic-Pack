			{
				if (card.cardtype == "jruler" or card.cardtype == "ruler" or card.cardtype == "stone" or card.wheeltype == "Inverse") then 90
				else if styling.name_font == "Reiya Cluster" then 83 else if styling.name_font == "Reiya Cluster" then 83 else 84
			}
		left: 
			{
				if (card.cardtype == "jruler" or card.cardtype == "ruler" or card.cardtype == "stone") then 67
