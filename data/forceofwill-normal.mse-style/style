mse version: 2.0.0
version: 2018-09-02
game: forceofwill
short name: Normal
full name: Regular Card
icon: ruler_icon.png
position hint: 1
installer group: fow/game/normal

depends on: 
	package: forceofwill.mse-game
depends on:
	package: forceofwill-symbols.mse-symbol-font
	
card width: 650
card height: 908
card dpi: 150
card background: rgb(255,255,255)

########################################

init script:
	full_art := "no"
	style := { styling.card_format }

########################################
	
styling field:
	type: choice
	name: card format
	description: Render the card in different styles
	choice: Lapis Cluster
	initial: Lapis Cluster
	
styling field:
	type: choice
	name: name font
	description: Render the card in different styles
	choice: Lapis Cluster
	choice: Reiya Cluster
	initial: Lapis Cluster

