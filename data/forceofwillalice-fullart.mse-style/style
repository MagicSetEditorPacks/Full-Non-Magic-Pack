mse version: 2.0.0
version: 2018-05-18
game: forceofwillalice
short name: Full Art
full name: Full Art Card
icon: ruler_icon.png
position hint: 23
installer group: fowalice/game/fullart

depends on: 
	package: forceofwillalice.mse-game
depends on:
	package: forceofwillalice-symbols.mse-symbol-font
	
card width: 650
card height: 908
card dpi: 150
card background: rgb(255,255,255)

########################################

init script:
	full_art := "yes"
	style := { styling.card_format }

########################################
	
styling field:
	type: choice
	name: card format
	description: Render the card in different styles
	choice: Alice Cluster
	initial: Alice Cluster

########################################

card style:
	
	cardtype:
		top:
			{
				if styling.card_format == "Lapis Cluster" then 562
				else 0
			}
