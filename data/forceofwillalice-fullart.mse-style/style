extra card style:
	cardshadow1:
		visible:
			{
				if styling.card_format == "Lapis Cluster" then true
				else if card.cardtype == "resonator" then true
				else if card.cardtype == "spell" then true
				else if card.cardtype == "regalia" then true
				else if card.cardtype != "resonator" then false
			}
		bottom:
			{
				if styling.card_format == "Lapis Cluster" and (card.cardtype == "jruler" or card.cardtype == "ruler" or card.cardtype == "stone") then 90
				else if styling.card_format == "Lapis Cluster" then 84
				else if card.cardtype == "ruler" then 827
				else if card.cardtype == "jruler" then 756
				else if card.cardtype == "resonator" then 74
				else if card.cardtype == "spell" then 74
				else if card.cardtype == "regalia" then 99
				else if card.cardtype == "stone" then 820
			}
		left: 
			{
				if card.cardtype == "resonator" then 172
				else if styling.card_format == "Lapis Cluster" and (card.cardtype == "jruler" or card.cardtype == "ruler" or card.cardtype == "stone") then 67
				else if card.cardtype == "spell" then 172
				else if card.cardtype == "stone" then 50
				else if card.cardtype == "ruler" then 50
				else if card.cardtype == "jruler" then 50
				else if card.cardtype == "regalia" then 172
			}
		width: 
			{
				if styling.card_format == "Lapis Cluster" and (card.cardtype == "jruler" or card.cardtype == "ruler" or card.cardtype == "stone") then 512
				else if card.cardtype == "ruler" then 442
				else if card.cardtype == "jruler" then 552
				else if card.cardtype == "resonator" then 430
				else if card.cardtype == "spell" then 430
				else if card.cardtype == "regalia" then 430
				else if card.cardtype == "stone" then 545
			}
		height: 40
		alignment: 
			{
				if styling.card_format == "Lapis Cluster" then "middle center stretch if-overflow"
				else if card.cardtype == "ruler" then "top center stretch if-overflow"
				else if card.cardtype == "jruler" then "top center stretch if-overflow"
				else if card.cardtype == "resonator" then "stretch if-overflow"
				else if card.cardtype == "spell" then "stretch if-overflow"
				else if card.cardtype == "regalia" then "stretch if-overflow"
				else if card.cardtype == "stone" then "top center stretch if-overflow"
			}
		font:
			name: Constantia
			size: 25
			color: black
			shadow color: 
				{
					if card.rarity == "SR" then "black"
					else if card.cardtype == "regalia" then "black"
					else if styling.card_format == "Lapis Cluster" then "black"
					else "white"
				}
			shadow displacement x: 1
			shadow displacement y: 1
			shadow blur: 0
		z index: 11
