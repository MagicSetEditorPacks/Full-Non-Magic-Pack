	cardshadow25:
		visible:
			{
				if styling.card_format == "Lapis Cluster" then true
				else if card.cardtype == "ruler" then false
				else if card.cardtype == "jruler" then false
				else if card.cardtype == "regalia" then false
				else if card.cardtype == "stone" then false
				else if card.cardtype == "spell" then false
				else true
			}
		top: 
			{
				if styling.card_format == "Lapis Cluster" then 562
				else if card.cardtype == "ruler" then 842
				else if card.cardtype == "stone" then 838
				else if card.cardtype == "jruler" then 771
				else if card.cardtype == "resonator" then (744 - card_rules_height() - card_flavor_height() - card_rules_flavor_gap(style: styling.card_format))
				else if card.cardtype == "spell" then 837
				else if card.cardtype == "regalia" then 837
			}
		left: 
			{
				if styling.card_format == "Lapis Cluster" and (card.cardtype == "ruler" or card.cardtype == "jruler") then 206
				else if styling.card_format == "Lapis Cluster" then 68
				else if card.cardtype == "ruler" then 242
				else if card.cardtype == "stone" then 50
				else if card.cardtype == "jruler" then 234
				else if card.cardtype == "resonator" then 54
				else if card.cardtype == "spell" then 37
				else if card.cardtype == "regalia" then 37
			}
		width: 
			{
				if styling.card_format == "Lapis Cluster" and (card.cardtype == "ruler" or card.cardtype == "jruler") then 260
				else if styling.card_format == "Lapis Cluster" then 342
				else if card.cardtype == "ruler" then 253
				else if card.cardtype == "stone" then 545
				else if card.cardtype == "jruler" then 361
				else if card.cardtype == "resonator" then 355
				else if card.cardtype == "spell" then 465
				else if card.cardtype == "regalia" then 465
			}
		height: 30
		alignment: 
			{
				if styling.card_format == "Lapis Cluster" then "stretch if-overflow"
				else if card.cardtype != "resonator" then "top center stretch if-overflow"
				else if card.cardtype == "resonator" then "stretch if-overflow"
			}
		font:
			name: Constantia
			size: {font_size_changer(type: card.cardtype, style: styling.card_format)}
			color: 
