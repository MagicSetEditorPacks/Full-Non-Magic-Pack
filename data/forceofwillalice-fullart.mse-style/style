	card_name:
		bottom:
			{
				if styling.card_format == "Lapis Cluster" and (card.cardtype == "jruler" or card.cardtype == "ruler" or card.cardtype == "stone") then 90
				else if styling.card_format == "Lapis Cluster" then 84
				else if card.cardtype == "ruler" then 827
				else if card.cardtype == "jruler" then 756
				else if card.cardtype == "resonator" then 74
				else if card.cardtype == "spell" then 74
				else if card.cardtype == "regalia" then 99
				else if card.cardtype == "stone" then 820
			}
		left: 
			{
				if card.cardtype == "resonator" then 172
				else if styling.card_format == "Lapis Cluster" and (card.cardtype == "jruler" or card.cardtype == "ruler" or card.cardtype == "stone") then 67
				else if card.cardtype == "spell" then 172
				else if card.cardtype == "stone" then 50
				else if card.cardtype == "ruler" then 50
				else if card.cardtype == "jruler" then 50
				else if card.cardtype == "regalia" then 172
			}
		width: 
