	cardRulesShadowDown:
		bottom: 
			{
				if styling.card_format == "Lapis Cluster" then (855 - card_flavor_height() - card_rules_flavor_gap(style: styling.card_format) - (if ceil(set.stylesheet.card_style.flavor.content_height) == 0 then 10))
				else if card.cardtype == "ruler" then (755 - card_flavor_height() - card_rules_flavor_gap(style: styling.card_format))
				else if card.cardtype == "stone" then (749 - card_flavor_height() - card_rules_flavor_gap(style: styling.card_format))
				else if card.cardtype == "jruler" then (683 - card_flavor_height() - card_rules_flavor_gap(style: styling.card_format))
				else if card.cardtype == "resonator" then (777 - card_flavor_height() - card_rules_flavor_gap(style: styling.card_format))
				else if card.cardtype == "spell" then (807 - card_flavor_height() - card_rules_flavor_gap(style: styling.card_format))
				else if card.cardtype == "regalia" then (807 - card_flavor_height() - card_rules_flavor_gap(style: styling.card_format))
			}
		left:
			{
				if styling.card_format == "Lapis Cluster" then 68
				else if (card.cardtype == "ruler") or
				(card.cardtype == "jruler") then 55
				else 53
			}
		alignment:
			{
				if styling.card_format == "Lapis Cluster" then "middle stretch if-overflow"
			}
		width: 
			{
				if styling.card_format == "Lapis Cluster" then 520
				else 544
			}
		height: 
			script:
				if styling.card_format == "Lapis Cluster" then (243 - card_flavor_height())
				else (max(19, card_rules_height()))
